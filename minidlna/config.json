{
  "name": "MiniDLNA",
  "version": "0.0.24",
  "slug": "minidlna",
  "description": "RedyMedia MiniDLNA daemon serves media files (music, pictures, and video) to clients on a network",
  "url": "https://github.com/bvlaicu/home-assistant-addons/tree/master/minidlna",
  "startup": "application",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "webui": "[PROTO:ssl]://[HOST]:[PORT:80]",
  "ingress": true,
  "ingress_port": 1337,
  "panel_icon": "mdi:chart-areaspline",
  "startup": "services",
  "homeassistant": "0.92.0b2",
  "arch": ["aarch64", "amd64", "armv7", "i386"],
  "map": ["share:rw", "ssl"],
  "boot": "auto",
  "hassio_api": true,
  "auth_api": true,
  "hassio_role": "default",
  "homeassistant_api": false,
  "ports": {
    "1900/udp": 1900,
    "8200/tcp": 8200
  },
  "ports_description": {
    "1900/udp": "MiniDLNA UDP server",
    "8200/tcp": "MiniDLNA HTTP server"
  },
  "options": {
    "log_level": "info"
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)"
  }
}

